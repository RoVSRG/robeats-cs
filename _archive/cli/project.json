{
  "name": "cli",
  "sourceRoot": "packages/cli/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/cli"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "packages/cli"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "packages/cli"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "packages/cli"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run start",
        "cwd": "packages/cli"
      }
    }
  },
  "tags": ["scope:cli", "type:application"]
}