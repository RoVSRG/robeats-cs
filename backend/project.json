{
  "name": "backend",
  "sourceRoot": "packages/backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["prisma:generate"],
      "options": {
        "command": "npm run build",
        "cwd": "packages/backend"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run up",
        "cwd": "packages/backend"
      }
    },
    "down": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run down",
        "cwd": "packages/backend"
      }
    },
    "logs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run logs",
        "cwd": "packages/backend"
      }
    },
    "dev:up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev:up",
        "cwd": "packages/backend"
      }
    },
    "dev:logs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev:logs",
        "cwd": "packages/backend"
      }
    },
    "rebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run rebuild",
        "cwd": "packages/backend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "packages/backend"
      }
    },
    "lint:check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint:check",
        "cwd": "packages/backend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "packages/backend"
      }
    },
    "prisma:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:generate",
        "cwd": "packages/backend"
      },
      "outputs": ["{projectRoot}/generated"]
    },
    "db:migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run db:migrate",
        "cwd": "packages/backend"
      }
    }
  },
  "tags": ["scope:backend", "type:application"]
}