{
  "name": "robeats-cs-scripts",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "ReplicatedStorage": {
      "$path": "src/shared",
      "$ignoreUnknownInstances": true,
      "Remotes": {
        "$className": "Folder",
        "Events": {
          "$className": "Folder",
          "$path": "src/shared/remotes/events"
        },
        "Functions": {
          "$className": "Folder",
          "$path": "src/shared/remotes/functions"
        }
      },
      "RobeatsGameCore": {
        "$path": "src/engine"
      }
    },

    "ServerScriptService": {
      "$path": "src/server",
      "$ignoreUnknownInstances": true
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    },

    "StarterGui": {
      "$ignoreUnknownInstances": true,
      "Screens": {
        "$path": "src/gui/screens",
        "$ignoreUnknownInstances": true
      }
    }
  }
}
