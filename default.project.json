{
  "name": "robeats-cs-scripts",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "ClockTime": 18.1
      }
    },

    "ReplicatedStorage": {
      "$path": "packages/game/shared",
      "$ignoreUnknownInstances": true,
      "Remotes": {
        "$className": "Folder",
        "Events": {
          "$className": "Folder",
          "$path": "packages/game/remotes/events"
        },
        "Functions": {
          "$className": "Folder",
          "$path": "packages/game/remotes/functions"
        },
        "Development": {
          "$className": "Folder",
          "$path": "packages/game/remotes/development"
        }
      },
      "Skins": {
        "$path": "packages/skins"
      },
      "Bindables": {
        "$path": "packages/game/bindables"
      },
      "RobeatsGameCore": {
        "$path": "packages/game/engine"
      },
      "State": {
        "$path": "packages/game/state"
      }
    },

    "ServerScriptService": {
      "$path": "packages/game/server"
    },

    "ServerStorage": {
      "$ignoreUnknownInstances": true,
      "Plugins": {
        "$className": "Folder",
        "$path": "packages/plugins"
      },
      "Songs": {
        "$className": "Folder",
        "$path": "packages/songs/dist"
      }
    },

    "Workspace": {
      "$ignoreUnknownInstances": true,
      "$path": "packages/game/workspace",
      "$properties": {
        "StreamingEnabled": false
      }
    },

    "Players": {
      "$properties": {
        "CharacterAutoLoads": false
      }
    },

    "StarterPlayer": {
      "$properties": {
        "LoadCharacterAppearance": false
      },
      "StarterPlayerScripts": {
        "$path": "packages/game/client"
      }
    },

    "StarterGui": {
      "$ignoreUnknownInstances": true,
      "Screens": {
        "$path": "packages/game/gui/screens",
        "$ignoreUnknownInstances": false
      }
    }
  }
}
