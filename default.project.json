{
  "name": "robeats-cs-scripts",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,

    "ReplicatedStorage": {
      "$path": "src/shared",
      "$ignoreUnknownInstances": true,
      "Remotes": {
        "$className": "Folder",
        "Events": {
          "$className": "Folder",
          "$path": "src/remotes/events"
        },
        "Functions": {
          "$className": "Folder",
          "$path": "src/remotes/functions"
        }
      },
      "Skins": {
        "$path": "src/skins"
      },
      "RobeatsGameCore": {
        "$path": "src/engine"
      },
      "State": {
        "$path": "src/state"
      }
    },

    "ServerScriptService": {
      "$path": "src/server"
    },

    "ServerStorage": {
      "$ignoreUnknownInstances": true,
      "Plugins": {
        "$className": "Folder",
        "$path": "plugins"
      },
      "Songs": {
        "$className": "Folder",
        "$path": "build"
      }
    },

    "Workspace": {
      "$ignoreUnknownInstances": true,
      "$path": "src/workspace",
      "$properties": {
        "StreamingEnabled": false
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    },

    "StarterGui": {
      "$ignoreUnknownInstances": true,
      "Screens": {
        "$path": "src/gui/screens"
      }
    }
  }
}
