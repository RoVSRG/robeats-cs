{
  "name": "backend",
  "sourceRoot": "packages/backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["prisma:generate"],
      "options": {
        "command": "npm run build",
        "cwd": "packages/backend"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "dependsOn": ["prisma:generate"],
      "options": {
        "command": "npm run dev",
        "cwd": "packages/backend"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "npm run start",
        "cwd": "packages/backend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "packages/backend"
      }
    },
    "lint:check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint:check",
        "cwd": "packages/backend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "packages/backend"
      }
    },
    "prisma:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prisma:generate",
        "cwd": "packages/backend"
      },
      "outputs": ["{projectRoot}/generated"]
    },
    "dev:up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev:up",
        "cwd": "packages/backend"
      }
    },
    "dev:down": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev:down",
        "cwd": "packages/backend"
      }
    },
    "prod:up": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prod:up",
        "cwd": "packages/backend"
      }
    },
    "prod:down": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prod:down",
        "cwd": "packages/backend"
      }
    },
    "prod:rebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prod:rebuild",
        "cwd": "packages/backend"
      }
    }
  },
  "tags": ["scope:backend", "type:application"]
}