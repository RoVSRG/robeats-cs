{
  "name": "songs",
  "sourceRoot": "packages/songs",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "lune run packages/songs/build.luau",
        "cwd": "."
      },
      "outputs": ["{projectRoot}/packages/songs/dist"]
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "packages/songs"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "lune run packages/songs/server",
        "cwd": "."
      }
    }
  },
  "tags": ["scope:songs", "type:library"]
}